@prefix occp: <http://www.semanticweb.org/albrechtvaatz/ontologies/2022/9/cMod_V0.1#> .
@prefix ould: <http://www.semanticweb.org/albrechtvaatz/ontologies/2024/OULD#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.example.de/example#component1> a ould:UpdatableEntity ;
    occp:hasCycle <http://www.example.de/example#cycleA1>,
        <http://www.example.de/example#cycleA2> ;
    occp:hasPhase <http://www.example.de/example#phaseA>,
        <http://www.example.de/example#phaseB>,
        <http://www.example.de/example#phaseC> ;
    ould:hasHash "abc123" ;
    ould:hasIFCID "ifc:Wall1" .

<http://www.example.de/example#instant_BeginningOfConstruction> a occp:BeginningOfConstruction ;
    occp:hasActualTime "2025-03-15"^^xsd:date ;
    occp:startsPhase <http://www.example.de/example#phaseC> .

<http://www.example.de/example#instant_CompletionOfConstruction> a occp:CompletionOfConstruction ;
    occp:endsPhase <http://www.example.de/example#phaseC> ;
    occp:hasActualTime "2025-06-30"^^xsd:date .

<http://www.example.de/example#cycleA1> a occp:CycleA_PlanningReview ;
    occp:hasActualBeginning <http://www.example.de/example#instant_BeginningOfPlanning> ;
    occp:hasActualEnd <http://www.example.de/example#instant_ReviewRejection> ;
    occp:hasCycleNumber 1 ;
    occp:hasEstimatedBeginning <http://www.example.de/example#instant_BeginningOfPlanning> ;
    occp:hasEstimatedEnd <http://www.example.de/example#instant_ReviewRejection> ;
    occp:isInPhase <http://www.example.de/example#phaseA> .

<http://www.example.de/example#cycleA2> a occp:CycleA_PlanningReview ;
    occp:hasActualBeginning <http://www.example.de/example#instant_SubmissionToReview> ;
    occp:hasActualEnd <http://www.example.de/example#instant_ReviewApproval> ;
    occp:hasCycleNumber 2 ;
    occp:hasEstimatedBeginning <http://www.example.de/example#instant_SubmissionToReview> ;
    occp:hasEstimatedEnd <http://www.example.de/example#instant_ReviewApproval> ;
    occp:isInPhase <http://www.example.de/example#phaseB> .

<http://www.example.de/example#phaseC> a occp:PhaseC_Construction .

<http://www.example.de/example#instant_BeginningOfPlanning> a occp:BeginningOfPlanning ;
    occp:hasActualTime "2025-01-01"^^xsd:date ;
    occp:startsCycle <http://www.example.de/example#cycleA1> ;
    occp:startsPhase <http://www.example.de/example#phaseA> .

<http://www.example.de/example#instant_ReviewApproval> a occp:ReviewApproval ;
    occp:endsCycle <http://www.example.de/example#cycleA2> ;
    occp:endsPhase <http://www.example.de/example#phaseB> ;
    occp:hasActualTime "2025-03-01"^^xsd:date .

<http://www.example.de/example#instant_ReviewRejection> a occp:ReviewRejection ;
    occp:endsCycle <http://www.example.de/example#cycleA1> ;
    occp:endsPhase <http://www.example.de/example#phaseA> ;
    occp:hasActualTime "2025-02-01"^^xsd:date .

<http://www.example.de/example#instant_SubmissionToReview> a occp:SubmissionToReview ;
    occp:hasActualTime "2025-02-15"^^xsd:date ;
    occp:startsCycle <http://www.example.de/example#cycleA2> ;
    occp:startsPhase <http://www.example.de/example#phaseB> .

<http://www.example.de/example#phaseA> a occp:PhaseA_Planning ;
    occp:hasActualBeginning <http://www.example.de/example#instant_BeginningOfPlanning> ;
    occp:hasActualEnd <http://www.example.de/example#instant_ReviewRejection> ;
    occp:hasEstimatedBeginning <http://www.example.de/example#instant_BeginningOfPlanning> ;
    occp:hasEstimatedEnd <http://www.example.de/example#instant_ReviewRejection> .

<http://www.example.de/example#phaseB> a occp:PhaseB_Review ;
    occp:hasActualBeginning <http://www.example.de/example#instant_SubmissionToReview> ;
    occp:hasActualEnd <http://www.example.de/example#instant_ReviewApproval> ;
    occp:hasEstimatedBeginning <http://www.example.de/example#instant_SubmissionToReview> ;
    occp:hasEstimatedEnd <http://www.example.de/example#instant_ReviewApproval> .

