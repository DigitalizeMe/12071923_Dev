      ] ;
    sh:property [                         # End date - either estimated or actual or both
      sh:path time:hasEnd ;
      sh:minCount 1 ;
      sh:message "Missing date for (estimated or actual) End in Phase F (DesignLife) - min. count = 1." ;
      sh:severity sh:Warning ;
      ] ;
    sh:property [                         # Estimated End date
