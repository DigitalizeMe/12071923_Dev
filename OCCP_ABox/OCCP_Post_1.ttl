@prefix occp: <http://www.semanticweb.org/albrechtvaatz/ontologies/2022/9/cMod_V0.1#> .
@prefix ould: <http://www.semanticweb.org/albrechtvaatz/ontologies/2024/OULD#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://www.example.de/example#> .

:instant_BeginningOfPlanning a occp:BeginningOfPlanning ;
    occp:hasActualTime "2025-01-01"^^xsd:date ;
    occp:startsPhase :phaseA ;
    occp:startsCycle :cycleA .

:component1 a ould:UpdatableEntity ;
    occp:hasPhase :phaseA ;
    occp:hasCycle :cycleA ;
    ould:hasIFCID "ifc:Wall1" ;
    ould:hasHash "abc123" .

:phaseA a occp:PhaseA_Planning ;
    occp:hasActualBeginning :instant_BeginningOfPlanning .

:cycleA a occp:CycleA_PlanningReview ;
    occp:hasCycleNumber 1 ;
    occp:isInPhase :phaseA ;  # Inferiert basierend auf BeginningOfPlanning
    occp:hasActualBeginning :instant_BeginningOfPlanning .