@prefix occp: <http://www.semanticweb.org/albrechtvaatz/ontologies/2022/9/cMod_V0.1#> .
@prefix ould: <http://www.semanticweb.org/albrechtvaatz/ontologies/2024/OULD#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://www.example.de/example#> .

# Component
ex:Wall_Component1 a ould:UpdatableEntity ;
    occp:hasPhase ex:phaseA_Planning ;
    occp:hasPhase ex:phaseB_Review ;
    occp:hasCycle ex:cycleA1 ;
    occp:hasCycle ex:cycleA2 ;
    occp:hasCycle ex:cycleA3 ;
    ould:hasIFCID "ifc:Wall_001" ;
    ould:hasHash "xyz789" .

# Phases
ex:phaseA_Planning a occp:PhaseA_Planning .
ex:phaseB_Review a occp:PhaseB_Review .

# Cycles
ex:cycleA1 a occp:CycleA_PlanningReview ;
    occp:hasCycleNumber 1 ;
    occp:isInPhase ex:phaseB_Review .

ex:cycleA2 a occp:CycleA_PlanningReview ;
    occp:hasCycleNumber 2 ;
    occp:isInPhase ex:phaseB_Review .

ex:cycleA3 a occp:CycleA_PlanningReview ;
    occp:hasCycleNumber 3 ;
    occp:isInPhase ex:phaseB_Review .

# Instants
ex:instant_BeginningOfPlanning a occp:BeginningOfPlanning ;
    occp:hasActualTime "2025-03-01"^^xsd:date ;
    occp:startsPhase ex:phaseA_Planning ;
    occp:startsCycle ex:cycleA1 .

ex:instant_DataProcurement a occp:DataProcurement ;
    occp:hasActualTime "2025-03-05"^^xsd:date ;
    occp:isPartOfPhase ex:phaseA_Planning ;
    occp:isPartOfCycle ex:cycleA1 .

ex:instant_Edit_BSTR a occp:Edit_BSTR ;
    occp:hasActualTime "2025-03-10"^^xsd:date ;
    occp:isPartOfPhase ex:phaseA_Planning ;
    occp:isPartOfCycle ex:cycleA1 .

ex:instant_SubmissionToReview1 a occp:SubmissionToReview ;
    occp:hasActualTime "2025-03-15"^^xsd:date ;
    occp:startsPhase ex:phaseB_Review ;  # Startet Phase B statt ReviewStart
    occp:startsCycle ex:cycleA1 .

ex:instant_ReviewStart1 a occp:ReviewStart ;
    occp:hasActualTime "2025-03-16"^^xsd:date ;
    occp:isPartOfPhase ex:phaseB_Review ;
    occp:isPartOfCycle ex:cycleA1 .

ex:instant_ReviewRejection1 a occp:ReviewRejection ;
    occp:hasActualTime "2025-03-20"^^xsd:date ;
    occp:endsCycle ex:cycleA1 ;
    occp:startsCycle ex:cycleA2 .

ex:instant_Edit_ASTR1 a occp:Edit_ASTR ;
    occp:hasActualTime "2025-03-25"^^xsd:date ;
    occp:isPartOfPhase ex:phaseA_Planning ;
    occp:isPartOfCycle ex:cycleA2 .

ex:instant_SubmissionToReview2 a occp:SubmissionToReview ;
    occp:hasActualTime "2025-03-30"^^xsd:date ;
    occp:startsCycle ex:cycleA2 .

ex:instant_ReviewStart2 a occp:ReviewStart ;
    occp:hasActualTime "2025-03-31"^^xsd:date ;
    occp:isPartOfPhase ex:phaseB_Review ;
    occp:isPartOfCycle ex:cycleA2 .

ex:instant_ReviewRejection2 a occp:ReviewRejection ;
    occp:hasActualTime "2025-04-05"^^xsd:date ;
    occp:endsCycle ex:cycleA2 ;
    occp:startsCycle ex:cycleA3 .

ex:instant_Edit_ASTR2 a occp:Edit_ASTR ;
    occp:hasActualTime "2025-04-10"^^xsd:date ;
    occp:isPartOfPhase ex:phaseA_Planning ;
    occp:isPartOfCycle ex:cycleA3 .

ex:instant_SubmissionToReview3 a occp:SubmissionToReview ;
    occp:hasActualTime "2025-04-15"^^xsd:date ;
    occp:startsCycle ex:cycleA3 .

ex:instant_ReviewStart3 a occp:ReviewStart ;
    occp:hasActualTime "2025-04-16"^^xsd:date ;
    occp:isPartOfPhase ex:phaseB_Review ;
    occp:isPartOfCycle ex:cycleA3 .

ex:instant_ReviewApproval a occp:ReviewApproval ;
    occp:hasActualTime "2025-04-20"^^xsd:date ;
